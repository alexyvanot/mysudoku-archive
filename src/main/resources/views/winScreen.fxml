<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="anchorPane" prefHeight="413.0" prefWidth="505.0" stylesheets="@winScreen.css" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.intech.mysudoku.controllers.WinScreenController">
   <children>
      <ImageView fitHeight="220.0" fitWidth="223.0" layoutX="177.0" layoutY="87.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/winCup.png" />
         </image>
      </ImageView>
      <Text fill="#0f9cee" layoutX="162.0" layoutY="91.0" strokeType="OUTSIDE" strokeWidth="0.0" text="YOU WON !">
         <font>
            <Font name="Arial Black" size="42.0" />
         </font>
      </Text>
      <Label layoutX="211.0" layoutY="297.0" text="Difficulty :" textFill="WHITE">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Button fx:id="replayButton" layoutX="211.0" layoutY="388.0" mnemonicParsing="false" onAction="#handleReplayGame" text="Replay">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="exitButton" layoutX="297.0" layoutY="388.0" mnemonicParsing="false" onAction="#showMainMenu" prefHeight="29.0" prefWidth="58.0" text="Quit">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Text fx:id="difficultyText" fill="#0f9cee" layoutX="320.0" layoutY="318.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
         <font>
            <Font name="Arial Black" size="18.0" />
         </font>
      </Text>
      <ImageView fitHeight="458.0" fitWidth="542.0" layoutX="19.0" layoutY="14.0" opacity="0.08" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../../../../Desktop/icon.png" />
         </image>
      </ImageView>
      <Text fx:id="chronoText" fill="#0f9cee" layoutX="247.0" layoutY="362.0" strokeType="OUTSIDE" strokeWidth="0.0" text="00:00:00">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
